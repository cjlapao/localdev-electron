{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/e2e",
    "module": "commonjs",
    "types": [
      "mocha",
      "node"
    ]
  },
  "include": [
    "**.ts"
  ],
  "exclude": [
    "node_modules",
    "app/node_modules",
    "./node_modules/",
    "./app/node_modules/",
    "node_modules/*",
    "app/node_modules/*",
    "**/node_modules/*",
    "**/app/node_modules/*"
  ]
}
